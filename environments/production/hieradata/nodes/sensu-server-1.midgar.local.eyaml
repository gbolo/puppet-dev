---
sensu::server: true
sensu::api: true
sensu::rabbitmq_host: 'sensu-rabbitmq-1.midgar.local'
sensu::rabbitmq_port: '5671'
sensu::rabbitmq_user: 'sensu'
sensu::rabbitmq_password: 'sensu'
sensu::rabbitmq_reconnect_on_error: true
sensu::rabbitmq_ssl: true
sensu::rabbitmq_ssl_cert_chain: 'puppet:///modules/profile/sensu_rabbitmq/chain.pem'
sensu::rabbitmq_ssl_private_key: ENC[PKCS7,]
sensu::redis_host: 'sensu-redis-1.midgar.local'
sensu::redis_port: '6379'
sensu::api_bind: "%{ipaddress}"
sensu::api_host: 'sensu-server-1.midgar.local'
#sensu::api_port: 
sensu::api_user: 'sensu'
sensu::api_password: 'sensu'


uchiwa::install_repo: false
uchiwa::host: "%{ipaddress}"
uchiwa::port: '3000'
uchiwa::sensu_api_endpoints:
  -
    host: 'sensu-server-1.midgar.local'
    insecure: false
    ssl: false
    user: 'sensu'
    pass: 'sensu'

